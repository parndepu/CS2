//  String test program
//
//  Tests: length capacity
//

#include "string.hpp"
#include <cassert>
#include <iostream>

int main()
{
    {
        // Setup
        String str1("hello");
        String str2(str1);

        // Verify
        assert(str1 == str2);
    }
    
    std::cout << "Done testing copy constructor" << std::endl;
}